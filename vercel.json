{
  "version": 2,
  "builds": [
    {
      "src": "ai-chat-hub/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/[^.]+",
      "dest": "ai-chat-hub/index.html",
      "status": 200
    },
    {
      "src": "/(.+)",
      "dest": "ai-chat-hub/$1"
    }
  ]
}